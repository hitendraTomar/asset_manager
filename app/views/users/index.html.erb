<div class="container">
	<p id="notice"><%= notice %></p>

	<h1>Houses</h1>

	<table>
	  <thead>
	    <tr>
	      <th>Owner</th>
	      <th>Address</th>
	      <th>Rooms</th>
	      <th>Sqmt</th>
	      <th>Floor</th>
	      <th>Air cond</th>
	      <th>Price</th>
	      <th colspan="3"></th>
	    </tr>
	  </thead>

	  <tbody>
	    <% @houses.each do |house| %>
	      <tr>
	        <td><%= house.email %></td>
	        <td><%= house.address %></td>
	        <td><%= house.rooms %></td>
	        <td><%= house.sqmt %></td>
	        <td><%= house.floor %></td>
	        <td><%= house.air_cond %></td>
	        <td><%= house.price %></td>
	        <% if current_user %>
	        	<td><%= link_to "BUY",  controller: "houses", action: "buy", id: house.id  %> </td>
		        <td><%= link_to 'Show', house %></td>
		        <td><%= link_to 'Edit', edit_house_path(house) %></td>
		        <td><%= link_to 'Destroy', house, method: :delete, data: { confirm: 'Are you sure?' } %></td>
		    <% end %>
	      </tr>
	    <% end %>
	  </tbody>
	</table>

	<br>

	<%= link_to 'New House', new_house_path %>


	<h1>Complex Building</h1>

	<table>
	  <thead>
	    <tr>
	      <th>Owner</th>
	      <th>Address</th>
	      <th>Units</th>
	      <th>Sqmt</th>
	      <th>Price</th>
	      <th colspan="3"></th>
	    </tr>
	  </thead>

	  <tbody>
	    <% @complex_buildings.each do |complex_building| %>
	      <tr>
	        <td><%= complex_building.email %></td>
	        <td><%= complex_building.address %></td>
	        <td><%= complex_building.units %></td>
	        <td><%= complex_building.sqmt %></td>
	        <td><%= complex_building.price %></td>
	        <% if current_user %>
	        	<td><%= link_to "BUY",  controller: "complex_buildings", action: "buy", id: complex_building.id  %> </td>
		        <td><%= link_to 'Show', complex_building %></td>
		        <td><%= link_to 'Edit', edit_complex_building_path(complex_building) %></td>
		        <td><%= link_to 'Destroy', complex_building, method: :delete, data: { confirm: 'Are you sure?' } %></td>
		    <% end %>
	      </tr>
	    <% end %>
	  </tbody>
	</table>

	<br>

	<%= link_to 'New Complex Building', new_complex_building_path %>


	<h1>Commecial Unit</h1>

	<table>
	  <thead>
	    <tr>
	      <th>Owner</th>
	      <th>Address</th>
	      <th>Shops</th>
	      <th>Sqmt</th>
	      <th>Parking</th>
	      <th>Price</th>
	      <th colspan="3"></th>
	    </tr>
	  </thead>

	  <tbody>
	    <% @commecial_units.each do |commecial_unit| %>
	      <tr>
	        <td><%= commecial_unit.email %></td>
	        <td><%= commecial_unit.address %></td>
	        <td><%= commecial_unit.shops %></td>
	        <td><%= commecial_unit.sqmt %></td>
	        <td><%= commecial_unit.parking %></td>
	        <td><%= commecial_unit.price %></td>
	        <% if current_user %>
	        	<td><%= link_to "BUY",  controller: "commecial_units", action: "buy", id: commecial_unit.id  %> </td>
		        <td><%= link_to 'Show', commecial_unit %></td>
		        <td><%= link_to 'Edit', edit_commecial_unit_path(commecial_unit) %></td>
		        <td><%= link_to 'Destroy', commecial_unit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
	        <% end %>
	      </tr>
	    <% end %>
	  </tbody>
	</table>

	<br>

	<%= link_to 'New Commecial Unit', new_commecial_unit_path %>

</div>
